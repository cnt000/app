{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.1",
    "@testing-library/user-event": "^7.2.1",
    "fastify": "^2.15.3",
    "fastify-static": "^2.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.0",
    "use-http": "^0.2.9"
  },
  "scripts": {
    "start": "npm run start:server && npm run start:client",
    "start:client": "react-scripts start",
    "start:server": "node src/server/index.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:unit": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "styleguide:copy": "cp -R src/styleguide build/",
    "storybook:copy": "cp -R storybook-static build/storybook",
    "styleguide": "while true; do npm run styleguide:copy;date ; sleep 5; done;",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/react": "^5.3.18",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "react-test-renderer": "^16.13.0"
  }
}
